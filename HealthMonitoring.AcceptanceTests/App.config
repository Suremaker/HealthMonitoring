      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Http.SelfHost" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-5.2.3.0" newVersion="5.2.3.0" />
